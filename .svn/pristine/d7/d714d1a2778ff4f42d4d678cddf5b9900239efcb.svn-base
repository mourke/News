import java.net.URL; 
import java.util.Date;

class Story extends Item {
  private Integer numberOfComments;
  private URL url;
  private String title;
  private Integer score;

  public Integer numberOfComments() {
    return numberOfComments;
  }

  public URL url() {
    return url;
  }

  public String title() {
    return title;
  }

  public Integer score() {
    return score;
  }

  Story(Integer numberOfComments, 
    URL url, 
    String title, 
    Integer score, 
    ArrayList<Integer> kids, 
    Date releaseDate, 
    Integer id, 
    String author, 
    Type type) {
    super(kids, releaseDate, id, author, type);
    this.numberOfComments = numberOfComments;
    this.url = url;
    this.title = title;
    this.score = score;
  }

  Story(JSONObject dictionary) throws Exception {
    super(dictionary);
    numberOfComments = dictionary.getInt("descendants");
    url = new URL(dictionary.getString("url"));
    title = dictionary.getString("title");
    score = dictionary.getInt("score");
  }
}
